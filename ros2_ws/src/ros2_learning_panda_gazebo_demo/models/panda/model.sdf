    <!-- ========================================================================= -->
    <!-- Plugins: 世界/模型插件 -->
    <!-- 这些插件给模型赋予了“超能力”，比如发布关节状态或响应控制命令。 -->
    <!-- ========================================================================= -->
    
    <!-- JointStatePublisher: 负责把每个关节的实时角度发布到 Gazebo Transport 话题上。 -->
    <!-- 这样 ros_gz_bridge 才能获取数据并转发给 ROS。 -->
    <plugin filename='gz-sim-joint-state-publisher-system' name='gz::sim::systems::JointStatePublisher'></plugin>

    <!-- JointPositionController: 关节位置控制器 -->
    <!-- 每个关节都需要一个控制器插件，用来接收目标位置命令，并输出力矩驱动关节运动。 -->
    <!-- topic: 监听的控制命令话题 (Gazebo Transport) -->
    <!-- p_gain/i_gain/d_gain: PID 控制参数，决定了运动有多“硬”或多“软”。 -->
    <plugin filename='gz-sim-joint-position-controller-system' name='gz::sim::systems::JointPositionController'>
      <joint_name>panda_joint1</joint_name>
      <topic>/panda/joint1/cmd_pos</topic>
      <p_gain>20</p_gain>
      <i_gain>0.1</i_gain>
      <d_gain>0.1</d_gain>
      <i_max>1</i_max>
      <i_min>-1</i_min>
      <cmd_max>2.9671</cmd_max>
      <cmd_min>-2.9671</cmd_min>
    </plugin>

