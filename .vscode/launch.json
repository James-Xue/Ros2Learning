        {
            "name": "ROS2: Debug talker (Debug build)",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/bash",
            "args": [
                "-lc",
                "cd '${workspaceFolder}/ros2_ws' && source ./scripts/source.sh jazzy && exec '${workspaceFolder}/ros2_ws/install/ros2_learning_cpp/lib/ros2_learning_cpp/talker'"
            ],
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "cwd": "${workspaceFolder}/ros2_ws",
            "externalConsole": false,
            "justMyCode": false,
            "sourceFileMap": {
                "./csu": "${workspaceFolder}/third_party/glibc/csu",
                "../csu": "${workspaceFolder}/third_party/glibc/csu",
                "./sysdeps": "${workspaceFolder}/third_party/glibc/sysdeps",
                "../sysdeps": "${workspaceFolder}/third_party/glibc/sysdeps",
                "./string/../sysdeps": "${workspaceFolder}/third_party/glibc/sysdeps",
                "../string/../sysdeps": "${workspaceFolder}/third_party/glibc/sysdeps",
                "./csu/../sysdeps": "${workspaceFolder}/third_party/glibc/sysdeps",
                "./csu/../csu": "${workspaceFolder}/third_party/glibc/csu",
                "/build/gcc-14-ig5ci0/gcc-14-14.2.0/build/x86_64-linux-gnu/libstdc++-v3/include": "/usr/include/c++/13",
                "/build/gcc-14-ig5ci0/gcc-14-14.2.0/build/x86_64-linux-gnu/libstdc++-v3/include/x86_64-linux-gnu": "/usr/include/x86_64-linux-gnu/c++/13"
            },
            "preLaunchTask": "ROS2: Prepare debug (jazzy)",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable debuginfod (auto-fetch debug symbols if available)",
                    "text": "-gdb-set debuginfod enabled on",
                    "ignoreFailures": true
                },
                {
                    "description": "Use Ubuntu debuginfod server by default",
                    "text": "-interpreter-exec console \"set debuginfod urls https://debuginfod.ubuntu.com\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Switch GDB working directory to repo root (so relative source paths work)",
                    "text": "-interpreter-exec console \"cd ${workspaceFolder}\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Add glibc source directory (for stepping into libc startup / ld.so)",
                    "text": "-interpreter-exec console \"directory third_party/glibc\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Load ROS2 source path mappings (rclcpp/rcutils) generated by task",
                    "text": "-interpreter-exec console \"source ${workspaceFolder}/.vscode/ros2_sources.gdbinit\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Map common glibc build-relative paths to local sources",
                    "text": "-interpreter-exec console \"set substitute-path ./csu third_party/glibc/csu\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Map common glibc build-relative paths to local sources",
                    "text": "-interpreter-exec console \"set substitute-path ./sysdeps third_party/glibc/sysdeps\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Map glibc string/../sysdeps paths to local sources",
                    "text": "-interpreter-exec console \"set substitute-path ./string/../sysdeps third_party/glibc/sysdeps\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Map libstdc++ distro build paths (std::string, etc.) to local headers",
                    "text": "-interpreter-exec console \"set substitute-path /build/gcc-14-ig5ci0/gcc-14-14.2.0/build/x86_64-linux-gnu/libstdc++-v3/include /usr/include/c++/13\"",
                    "ignoreFailures": true
                },
                {
                    "description": "Map libstdc++ arch-specific headers to local headers",
                    "text": "-interpreter-exec console \"set substitute-path /build/gcc-14-ig5ci0/gcc-14-14.2.0/build/x86_64-linux-gnu/libstdc++-v3/include/x86_64-linux-gnu /usr/include/x86_64-linux-gnu/c++/13\"",
                    "ignoreFailures": true
                }
            ]
        },
